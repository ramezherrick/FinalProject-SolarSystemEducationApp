
@{
    ViewData["Title"] = "ResultsPlanetsQuiz";
}

<h1>ResultsPlanetsQuiz</h1>

@model List<string>
@{int count = 0;}

<h1>Quiz: @ViewBag.englishname</h1>
<h2>your grade is @ViewBag.grade percent</h2>

@foreach (Questions question in ViewBag.questions)
{
<h4>@question.Question</h4>
<h4>Your answer:@Model[count] The correct answer is:@ViewBag.correctanswers[count]</h4>
<br />
    count++;
}